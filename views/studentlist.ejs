<%- include('./base/header.ejs')  %>
 <div class="container">
    <div class="row">
        <h4 class="text-center my-3" style="background-color: red;">Student List</h4>
        <table class="table text-center" >
            <thead>
                <tr>
                <th style="border-radius:0%;border:2px solid black;">Roll No</th>
                <th style="border-radius:0%;border:2px solid black;">First Name</th>
                <th style="border-radius:0%;border:2px solid black;">Last Name</th>
                <th style="border-radius:0%;border:2px solid black;">Father Name</th>
                <th style="border-radius:0%;border:2px solid black;">Adhar Card</th>
                <th style="border-radius:0%;border:2px solid black;">Mobile No</th>
                </tr>
            </thead>
            <% students.forEach((student)=>{ %>
                <tr>
                    <td><%= student.rollNo %></td>
                    <td><%= student.firstName %></td>
                    <td><%= student.lastName %></td>
                    <td><%= student.fatherName %></td>
                    <td><%= student.adharCardNo %></td>
                    <td><%= student.mobileNo %></td>
                </tr>
          <%  }) %>
        </table>
    </div>
 </div>
<%- include('./base/footer.ejs') %>
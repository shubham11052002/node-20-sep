<%- include('./base/header.ejs')  %>
 <div class="container">
    <div class="row">
        <h4 class="text-center my-3" style="background-color: red;">Student List</h4>
        <table class="table text-center" >
            <thead>
                <tr>
                <th>Roll No</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Father Name</th>
                <th>Adhar Card</th>
                <th>Mobile No</th>
                <th>Actions</th>
                </tr>
            </thead>
            <% students.forEach((student)=>{ %>
                <tr>
                    <td><%= student.rollNo %></td>
                    <td><%= student.firstName %></td>
                    <td><%= student.lastName %></td>
                    <td><%= student.fatherName %></td>
                    <td><%= student.adharCardNo %></td>
                    <td><%= student.mobileNo %></td>
                    <td ><a href="/edit/student/page/<%= student._id%>" class="btn btn-warning w-75" >Edit</a></td>
                </tr>
          <%  }) %>
        </table>
    </div>
 </div>
<%- include('./base/footer.ejs') %>